Algoritmo "2_SOMA_DE_1_ATE_15_NUMEROS"
Var
   i,total:inteiro
Inicio
   escreval("***Somar os 15 primeiros números***")
   escreval("___________________________________")
   escreval("")
   para i de 1 ate 15 faca
      total<- total + i
   fimpara
   escreval("O resultado da soma é:",total)
Fimalgoritmo