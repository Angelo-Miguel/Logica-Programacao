Algoritmo "5_SOMA_10_NUMEROS"
Var
   i,n,total:inteiro
Inicio
   escreval("***Somar 10 números***")
   escreval("______________________")
   escreval("")
   escreval("Informe 10 números")
   para i de 1 ate 10 faca
      leia(n)
      total<- total + n
   fimpara
   limpatela
   escreval("O resultado da soma é:",total)
Fimalgoritmo