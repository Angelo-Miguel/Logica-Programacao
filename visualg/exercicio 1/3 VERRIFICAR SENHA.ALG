Algoritmo "verrificar senha"
Var
   // Seção de Declarações das variáveis
   senha:inteiro

Inicio
   // Seção de Comandos, procedimento, funções, operadores, etc...
   escreval("***")
   escreval("Informe a senha")
   leia(senha)


   se(senha = 1234)entao
      escreva("SENHA CORRETA")
   senao
      escreva("SENHA INCORRETA")
   fimse
Fimalgoritmo